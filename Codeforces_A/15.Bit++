#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    cin>>n;
 
    string str;
 
    int count=0;
 
    for( i=0; i<n; i++)
    {
        cin>>str;
        if(str=="X++" || str=="++X")
        {
            count++;
        }
 
        else count--;
 
    }
 
    cout<<count;
 
    return 0;
 
}
